# =============================================================================
# BLOOCUBE AI SERVICES - PRODUCTION ENVIRONMENT CONFIGURATION
# =============================================================================

# Service Configuration
AI_SERVICE_NAME=bloocube-ai-service
AI_SERVICE_VERSION=1.0.0
AI_SERVICE_PORT=8080
AI_SERVICE_HOST=0.0.0.0

# Environment
NODE_ENV=production
ENVIRONMENT=production
LOG_LEVEL=info

# CORS Configuration
ALLOWED_CORS_ORIGINS=https://bloocube.com,https://admin.bloocube.com,https://api-backend.bloocube.com,https://api-ai-services.bloocube.com
ALLOWED_HOSTS=bloocube.com,admin.bloocube.com,api-backend.bloocube.com,api-ai-services.bloocube.com,localhost,127.0.0.1

# AI Model Configuration
PRIMARY_AI_PROVIDER=openai

# Vector Database Configuration
# Use FAISS as default (no configuration needed)

# Cloud Run Specific
PORT=8080
UVICORN_WORKERS=1

# Development
DEBUG=false
RELOAD=false


